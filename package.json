{
  "name": "sakuya",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "---db"   : "-- DB Script ------------------------------------------------------------",
    "startdb" : "docker-compose -f docker/postgresql.yml --env-file ../.env.local up -d",
    "stopdb"  : "docker-compose -f docker/postgresql.yml --env-file ../.env.local down",
    "logindb" : "docker exec -it postgresql bash",
    "cleandb" : "npm run stopdb && rm -rf docker/.pgsql_data",
    "---dev"  : "-- Dev Script -----------------------------------------------------------",
    "prestart": "npm run startdb",
    "start"   : "next dev",
    "---prd"  : "-- Prd Script -----------------------------------------------------------",
    "build"   : "next build",
    "startprd": "next start",
    "---util" : "-- Util Script ----------------------------------------------------------",
    "clean"   : "rm -rf .next",
    "cleanall": "npm run clean && npm run cleandb",
    "---end"  : "-------------------------------------------------------------------------"
  },
  "dependencies": {
    "@types/pg-promise": "^5.4.3",
    "next": "10.0.9",
    "pg-promise": "^10.9.5",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "sql-template-strings": "^2.2.2"
  },
  "devDependencies": {
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "typescript": "^4.2.3"
  }
}
